{
	"$schema": "https://opencode.ai/config.json",
	"theme": "system",
	"plugin": ["oh-my-opencode", "opencode-antigravity-auth@1.2.8"],
	"mcp": {
		"mcp_sequential_thinking": {
			"type": "local",
			"command": [
				"bunx",
				"-y",
				"@modelcontextprotocol/server-sequential-thinking"
			]
		},
		"mcp_exa": {
			"type": "local",
			"command": [
				"bunx",
				"-y",
				"exa-mcp-server",
				"tools=web_search_exa,get_code_context_exa,crawling_exa"
			]
		},
		"mcp_github": {
			"type": "remote",
			"url": "https://api.githubcopilot.com/mcp/",
			"headers": {
				"Authorization": "Bearer {file:~/.config/opencode/.secret/github_token}"
			}
		},
		"mcp_notion": {
			"type": "local",
			"command": ["bunx", "-y", "@notionhq/notion-mcp-server"],
			"environment": {
				"NOTION_TOKEN": "{file:~/.config/opencode/.secret/notion_token}"
			}
		},
		"mcp_anki": {
			"type": "remote",
			"url": "http://localhost:3141"
		}
	},
	"provider": {
		"proxy-api": {
			"npm": "@ai-sdk/openai-compatible",
			"name": "Proxy API",
			"options": {
				"baseURL": "http://127.0.0.1:8317/v1",
				"apiKey": "ak-12345678",
				"headers": {
					"Authorization": "Bearer ak-12345678"
				}
			},
			"models": {
				"gemini-3-flash-preview": {
					"name": "Gemini 3 Flash Preview [Antigravity]"
				},
				"gemini-3-pro-preview": {
					"name": "Gemini 3 Pro Preview [Antigravity]"
				},
				"gemini-claude-sonnet-4-5": {
					"name": "Gemini Claude Sonnet 4.5 [Antigravity]"
				},
				"gpt-oss-120b-medium": {
					"name": "GPT OSS 120b Medium [Antigravity]"
				},
				"gemini-claude-sonnet-4-5-thinking": {
					"name": "Gemini Claude Sonnet 4.5 Thinking [Antigravity]"
				},
				"gemini-claude-opus-4-5-thinking": {
					"name": "Gemini Claude Opus 4.5 Thinking [Antigravity]"
				},
				"gemini-2.5-flash": {
					"name": "Gemini 2.5 Flash [Antigravity]"
				},
				"gemini-2.5-flash-lite": {
					"name": "Gemini 2.5 Flash Lite [Antigravity]"
				}
			}
		},
		"google": {
			"models": {
				"antigravity-gemini-3-pro-low": {
					"name": "Gemini 3 Pro Low (Antigravity)",
					"limit": { "context": 1048576, "output": 65535 },
					"modalities": {
						"input": ["text", "image", "pdf"],
						"output": ["text"]
					}
				},
				"antigravity-gemini-3-pro-high": {
					"name": "Gemini 3 Pro High (Antigravity)",
					"limit": { "context": 1048576, "output": 65535 },
					"modalities": {
						"input": ["text", "image", "pdf"],
						"output": ["text"]
					}
				},
				"antigravity-gemini-3-flash": {
					"name": "Gemini 3 Flash (Antigravity)",
					"limit": { "context": 1048576, "output": 65536 },
					"modalities": {
						"input": ["text", "image", "pdf"],
						"output": ["text"]
					}
				},
				"gemini-3-pro-low": {
					"name": "Gemini 3 Pro Low (Gemini)",
					"limit": { "context": 1048576, "output": 65535 },
					"modalities": {
						"input": ["text", "image", "pdf"],
						"output": ["text"]
					}
				},
				"gemini-3-pro-high": {
					"name": "Gemini 3 Pro High (Gemini)",
					"limit": { "context": 1048576, "output": 65535 },
